    "util": "^0.12.1"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.6",
    "@types/q": "^1.5.2",
    "@types/react": "~16.8.2",
    "@types/react-dom": "~16.8.0",
    "base64-inline-loader": "^1.1.1",
    "css-loader": "~1.0.0",
    "file-loader": "~2.0.0",
    "rimraf": "~2.6.2",
    "sync-request": "^6.1.0",
    "tfx-cli": "^0.6.3",
    "ts-loader": "~5.2.2",
    "typescript": "^2.9.2"
  }
